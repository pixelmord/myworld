{
  "name": "@pixelmord/myworld",
  "version": "1.4.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@theme-ui/preset-tailwind": "^0.3.0",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.2",
    "next": "9.4.0",
    "next-tinacms-github": "^0.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^12.2.1",
    "react-tinacms-github": "^0.3.1",
    "react-tinacms-inline": "^0.4.9",
    "styled-components": "^5.1.0",
    "theme-ui": "^0.3.1",
    "tinacms": "^0.19.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^14.0.1",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/semantic-release": "^17.0.1",
    "@types/styled-components": "^5.1.0",
    "@types/theme-ui": "^0.3.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "typescript": "^3.9.2"
  }
}
